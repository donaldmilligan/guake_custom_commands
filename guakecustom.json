                ]
            },
            {
                "description": "git commit",
                "cmd": [
                    "git commit"
                ]
            },
            {
                "description": "git init",
                "cmd": [
                    "git init"
                ]
            },
            {
		"type": "menu",
		"description": "Git commit message",
		"items": [
		    {
		        "description": "write",
		        "cmd": [
		            "xdotool key --clearmodifiers ctrl+o"
		        ]
		    },
		    {
		        "description": "exit",
		        "cmd": [
		            "xdotool key --clearmodifiers ctrl+x"
		        ]
		    },
		    {
		        "description": "git init",
		        "cmd": [
		            "git init"
		        ]
		    }
        	]
    	    }
        ]
     },
     {
        "type": "menu",
        "description": "STATUS",
        "items": [
            {
                "description": "Running Servies",
                "cmd": [
                    "ls",
                    "-A"
                ]
            },
            {
                "description": "Net Connected",
                "cmd": [
                    "netstat -anp | grep -e tcp -e udp"
