        "description": "Save",
        "items": [
            {
                "description": "git add ?",
                "cmd": [
                    "echo 'enter file name or . for all files'; read addM; if [[ $addM ]]; then git add '$addM' && echo 'git add ${addM}'; unset addM ;else echo 'no parameters set';fi"
                ]
            },
            {
                "description": "git add .",
                "cmd": [
                    "git add ."
                ]
            },
            {
                "description": "git commit",
                "cmd": [
                    "git commit"
                ]
            },
            {
                "description": "push origin master",
                "cmd": [
                    "git push origin master"
                ]
            }
            ]
            },
            
            {
        "type": "menu",
        "description": "SPECIAL",
        "items": [
            {
                "description": "commit -am",
                "cmd": [

                    "echo 'Be sure to enter commit message'; read commitM; git add . ; git commit -am '$commitM'; echo $commitM && unset commitM;"
                ]
            },
            {
                "description": "fetch",
                "cmd": [
                    "git fetch"
                ]
            },
            {
